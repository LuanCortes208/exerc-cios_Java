package resultado;
import java.util.Scanner;

public class main2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.print("Este programa tem como objetivo definir se um número é par ou ímpar.");
		System.out.print("Por favor, digite um número: ");
		sc.nextInt();

		sc.close();
	}

}
